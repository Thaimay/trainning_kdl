  "main": "index.js",
  "scripts": {
    "start": "npm-run-all -p watch:*",
    "build:sass": "node-sass --importer node_modules/node-sass-globbing/index.js src/stylesheet/mycss.scss ../src/main/resources/static/css/mycss.css",
